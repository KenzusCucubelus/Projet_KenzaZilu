<!DOCTYPE html>
<html>
<head>
<meta charset="utf-8"> 
<title>Posts</title>
<style>
table {
    border-collapse: collapse;
}

table, td, th {
    border: 1px solid black;
}
</style>
</head>
<body>

<table>
  <tr>
    <th>Title</th>
    <th>Creation Time</th>
    <th>Description</th>
    <th>State</th>
  </tr>

  <%
  if(postData.length!=0){
  var i=1;
  postData.forEach(function(data){
  %>
  <tr>
    <td><%=i; %></td>
    <td><%=data.title%></td>
    <td><%=data.create_time%></td>
    <td><%=data.description%></td>
    <td><%=data.state%></td>
  </tr>
  <%  i++; }) %>
        <% } else{ %>
            <tr>
                <td colspan="7">No Data Found</td>
            </tr>
        <% } %>
</table>

</body>
</html>